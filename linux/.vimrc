:set nu
:noh
set ignorecase
set shortmess=atI
syntax on
autocmd InsertLeave * se nocul
set ruler
set showcmd
set foldenable
set foldmethod=manual
set autowrite
set cursorline
set confirm
set autoindent
set cindent
set tabstop=4
set softtabstop=4
set shiftwidth=4
set noexpandtab
set smarttab
set hlsearch
set incsearch
set enc=utf-8
set fencs=utf-8,ucs-bom,shift-jis,gb18030,gbk,gb2312,cp936
set langmenu=zh_CN.UTF-8
set showmatch
set smartindent
